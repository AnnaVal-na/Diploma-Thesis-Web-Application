# Секретный ключ Django (сгенерируйте новый в продакшене)
SECRET_KEY=your-secret-key-here

# Режим отладки (False в продакшене)
DEBUG=True

# Разрешённые хосты (через запятую)
ALLOWED_HOSTS=localhost,127.0.0.1